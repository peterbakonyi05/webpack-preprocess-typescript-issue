{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"declaration": false,
		"noImplicitReturns": true,
		"noImplicitAny": false,
		"noFallthroughCasesInSwitch": true,
		"noEmitOnError": true
	},
	"exclude": [
		"node_modules",
		"dist"
	]
}